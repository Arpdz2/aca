<script>
$(function(){
var url = window.location.href;
var page = url.substr(url.lastIndexOf('/')+1);
    if(page == "") {
        page = "/";
        $('a[href="' + page + '"]').addClass('active');
    }
    else {
        $('a[href*="' + page + '"]').addClass('active');
    }
});
</script>
